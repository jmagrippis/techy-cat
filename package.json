{
	"name": "techy-cat",
	"version": "0.2.0",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"test": "vitest",
		"test:e2e": "npx playwright test",
		"test:e2e:debug": "PWDEBUG=1 SLOW_MO=50 npx playwright test --project=chromium",
		"generate": "export $(grep -v '^#' .env | xargs) && openapi-typescript \"$VITE_SUPABASE_URL/rest/v1/?apikey=$VITE_SUPABASE_ANON_KEY\" --output src/lib/supabase.d.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.23.3",
		"@poppanator/sveltekit-svg": "^0.3.4",
		"@supabase/supabase-js": "^1.35.3",
		"@sveltejs/adapter-vercel": "next",
		"@sveltejs/kit": "next",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/svelte": "^3.1.3",
		"@testing-library/user-event": "^14.2.1",
		"@types/dotenv-flow": "^3.2.0",
		"@typescript-eslint/eslint-plugin": "^5.28.0",
		"@typescript-eslint/parser": "^5.28.0",
		"autoprefixer": "^10.4.7",
		"dotenv-flow": "^3.2.0",
		"eslint": "^8.18.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsdom": "^20.0.0",
		"jwt-decode": "^3.1.2",
		"openapi-typescript": "^5.4.0",
		"postcss": "^8.4.14",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"prettier-plugin-tailwindcss": "^0.1.11",
		"svelte": "^3.48.0",
		"svelte-check": "^2.7.2",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.3",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"unique-names-generator": "^4.7.1",
		"vitest": "^0.15.1",
		"vitest-svelte-kit": "^0.0.6"
	},
	"type": "module",
	"engines": {
		"node": "16.x"
	},
	"volta": {
		"node": "16.15.0"
	},
	"dependencies": {
		"stripe": "^9.12.0"
	}
}
